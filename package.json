{
  "name": "gassian",
  "version": "0.3.4",
  "description": "Tiny API wrapper for Atlassian Public Analytics aka GAS",
  "main": "dist/gassian.cjs.js",
  "module": "dist/gassian.esm.js",
  "browser": "dist/gassian.umd.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "start": "rollup -c -w",
